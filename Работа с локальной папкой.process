      <Content>&lt;ToolboxItemSettings DisplayName="Запустить робота" InternalName="StartBackgroundRobot" FriendlyName="Блок запускает ещё один экземпляр робота в качестве основного или фонового процесса, в том числе с возможностью запуска нескольких фоновых процессов параллельно. При запуске указывается его уникальное имя (чтобы затем отличить один запущенный экземпляр робота от другого) и файл .robot со сценарием, который необходимо выполнить. Блок может как ожидать завершения запущенного в новом экземпляре робота сценария (синхронный запуск), так и сразу продолжать выполнение текущего сценария (асинхронный, фоновый запуск). Текущий сценарий может содержать несколько блоков для асинхронного запуска роботов, что позволяет запустить несколько параллельно выполняющихся экземпляров роботов и сценариев. В случае асинхронного запуска проверить статус запущенного фонового сценария робота можно с помощью блока &amp;quot;Статус робота&amp;quot;. " Width="150" Height="100" PathToImage="" PathToToolboxImage="ExternalToolboxes\Image\ico_StartBackgroundRobot.png" ImageType="TopImage" NoDelete="False" Invisible="False" Proportional="True" Version="74" LinuxAvailable="True" Code="&amp;#xA;function StartBackgroundRobot{{{block_id}}}&amp;#xA;{        &amp;#xA; try&amp;#xA; {&amp;#xA;    $__Error.Enter(&amp;quot;{{{block_native_id}}}&amp;quot;, &amp;quot;{{{ErrorLevel}}}&amp;quot;,&amp;quot;{{{ErrorDirection}}}&amp;quot;,&amp;quot;{{{ReportLevel}}}&amp;quot;,&amp;quot;{{{ErrorMessage}}}&amp;quot;);&amp;#xA;    $__Error.AddConnectors(&amp;quot;{{{connector-Error}}}&amp;quot;, &amp;quot;{{{connectorRight}}}&amp;quot;, &amp;quot;{{{connectorBottom}}}&amp;quot;);&amp;#xA;    &amp;#xA;    $NameProcess   = {{{NameProcess}}};&amp;#xA;    $NameFileRobot = {{{NameFileRobot}}};&amp;#xA;    $WaitForExit   = ${{{WaitForExit}}};  &amp;#xA;    $WaitTimeObj   = {{{WaitTime}}};   &amp;#xA;    $LaunchMode    = &amp;quot;{{{LaunchMode}}}&amp;quot;;&amp;#xA;    &amp;#xA;        &amp;#xA;        $NameFileRobot = ExpandConstFilePath $NameFileRobot&amp;#xA;               &amp;#xA;        if(-not [System.IO.File]::Exists($NameFileRobot))&amp;#xA;        {&amp;#xA;            $__Error.Exception(&amp;quot;ERRORCONST_PATHNOTFOUND: $NameFileRobot&amp;quot;);&amp;#xA;            return;&amp;#xA;        } &amp;#xA;        &amp;#xA;        $ext = [System.IO.Path]::GetExtension($NameFileRobot).ToLower();&amp;#xA;        if($ext -ne &amp;quot;.robot&amp;quot;)&amp;#xA;        {&amp;#xA;            $__Error.Exception(&amp;quot;Unknown format. The .robot file is expected&amp;quot;);&amp;#xA;            return;&amp;#xA;        }&amp;#xA;                   &amp;#xA;        $toname = &amp;quot;{{{block_id}}}&amp;quot;;&amp;#xA;        &amp;#xA;        $RobotArguments      = $toname + &amp;quot;.json&amp;quot;;&amp;#xA;        $RobotArguments      = [System.IO.Path]::Combine($__WorkPath, $RobotArguments); &amp;#xA;        $valueRobotArguments = '{{{ProcessArguments}}}'.Trim(&amp;quot;&amp;quot;&amp;quot;&amp;quot;);&amp;#xA;        $valueRobotArguments = [System.Web.HttpUtility]::HtmlDecode($valueRobotArguments);&amp;#xA;        [System.IO.File]::WriteAllText($RobotArguments, $valueRobotArguments, [System.Text.Encoding]::UTF8);&amp;#xA;                     &amp;#xA;        #ProcessedRobotArgumentsIn&amp;#xA;                &amp;#xA;        if($LaunchMode -eq &amp;quot;Background&amp;quot;)&amp;#xA;        {&amp;#xA;            if([string]::IsNullOrEmpty($NameProcess))&amp;#xA;            {&amp;#xA;                $NameProcess = &amp;quot;{{{block_id}}}&amp;quot;;&amp;#xA;            }&amp;#xA;            &amp;#xA;            $args = &amp;quot;-unattended -background &amp;quot;&amp;quot;$NameProcess&amp;quot;&amp;quot; -robotarguments &amp;quot;&amp;quot;$RobotArguments&amp;quot;&amp;quot; &amp;quot;&amp;quot;$NameFileRobot&amp;quot;&amp;quot;&amp;quot;;&amp;#xA;                   &amp;#xA;            $startinfo                  = [System.Diagnostics.ProcessStartInfo]::new();&amp;#xA;            $startinfo.FileName         = $__RobotPath;&amp;#xA;            $startinfo.Arguments        = $args;&amp;#xA;            $startinfo.WorkingDirectory = [System.IO.Path]::GetDirectoryName($__RobotPath);&amp;#xA;&amp;#xA;            $proc   = [System.Diagnostics.Process]::Start($startinfo);&amp;#xA;            $procId = $proc.Id;         &amp;#xA;        }&amp;#xA;        elseif($LaunchMode -eq &amp;quot;Foreground (PiP)&amp;quot;)&amp;#xA;        {            &amp;#xA;            $valFile     = _shield $NameFileRobot; &amp;#xA;            $valArgs     = _shield $RobotArguments;&amp;#xA;            $psresultstr = Read-Host(&amp;quot;&amp;lt;PiPClient&amp;gt;&amp;lt;CommandName&amp;gt;LaunchScript&amp;lt;/CommandName&amp;gt;&amp;lt;NameFileRobot&amp;gt;$valFile&amp;lt;/NameFileRobot&amp;gt;&amp;lt;RobotArguments&amp;gt;$valArgs&amp;lt;/RobotArguments&amp;gt;&amp;lt;/PiPClient&amp;gt;&amp;quot;);                       &amp;#xA;            $psResult    = [MainHelpers.PSResult]::Deserialize($psresultstr);&amp;#xA;            if($__Error.CheckResult($psResult)) { return; }  &amp;#xA;                        &amp;#xA;            $procId = _toint($psResult.Result);  &amp;#xA;        }  &amp;#xA;        &amp;#xA;        {{{RobotID}}} = [MainHelpers.ProcessID]::CreatePID($procId).Serialize();&amp;#xA;        if($WaitForExit)&amp;#xA;        {           &amp;#xA;            $WaitTime = $WaitTimeObj * 1000;&amp;#xA;            if($WaitTime -le 0) { $WaitTime = -1; }&amp;#xA;            &amp;#xA;            try           &amp;#xA;            {&amp;#xA;                $proc = [System.Diagnostics.Process]::GetProcessById($procId);&amp;#xA;                if($null -ne $proc)&amp;#xA;                {&amp;#xA;                    $proc.WaitForExit($WaitTime); &amp;#xA;                    &amp;#xA;                    #ProcessedRobotArgumentsOut&amp;#xA;                }&amp;#xA;            }&amp;#xA;            catch{} &amp;#xA;        }&amp;#xA;        &amp;#xA;    }&amp;#xA;    catch &amp;#xA;    { &amp;#xA;        $__Error.ExceptionCatch($_);&amp;#xA;        return;&amp;#xA;    }    &amp;#xA;    $__Error.Quit();&amp;#xA;}&amp;#xA;" TextColor="#FFDADADA" ProtoName="" Language="" UrlVideo="{x:Null}" DerivedFrom="" NoResize="False" Container="False" MouseOverColor="Gray" MouseOverFill="Transparent" IsExpressionOnPause="False" ExpressionOnPause="" IsCollapsedBlock="False" Breakpoint="False" DirivedFrom="" xmlns="clr-namespace:ToolboxDesigner.Core;assembly=DiagramDesigner" xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:s="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"&gt;&lt;ToolboxItemSettings.PathStyle&gt;&lt;av:Style TargetType="av:Path"&gt;&lt;av:Style.BasedOn&gt;&lt;av:Style TargetType="av:Path"&gt;&lt;av:Style.Resources&gt;&lt;av:ResourceDictionary /&gt;&lt;/av:Style.Resources&gt;&lt;av:Setter Property="av:Shape.Fill"&gt;&lt;av:Setter.Value&gt;&lt;av:SolidColorBrush&gt;#FF252525&lt;/av:SolidColorBrush&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:Shape.Stroke"&gt;&lt;av:Setter.Value&gt;&lt;av:SolidColorBrush&gt;#FF41B1E1&lt;/av:SolidColorBrush&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:Shape.StrokeThickness"&gt;&lt;av:Setter.Value&gt;&lt;s:Double&gt;2&lt;/s:Double&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:Shape.StrokeLineJoin"&gt;&lt;av:Setter.Value&gt;&lt;x:Static Member="av:PenLineJoin.Round" /&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:Shape.Stretch"&gt;&lt;av:Setter.Value&gt;&lt;x:Static Member="av:Stretch.Fill" /&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:UIElement.IsHitTestVisible"&gt;&lt;av:Setter.Value&gt;&lt;s:Boolean&gt;False&lt;/s:Boolean&gt;&lt;/av:Setter.Value&gt;&lt;/av:Setter&gt;&lt;av:Setter Property="av:UIElement.SnapsToDevicePixels"&gt;&lt;av:Setter.Value&gt;&lt;s:Boolean&gt;True&lt;/s:Boolean&gt